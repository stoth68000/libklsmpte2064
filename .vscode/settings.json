        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/.lo": true,
        "**/*.lo": true,
        "**/*.Po": true,
        "**/*.la": true,
        "**/*.o": true,
        "**/Makefile": true,
        "**/Makefile.in": true,
        "**/.libs": true,
        "**/.deps": true,
        "**/m4": true,
        "**/autom4te.cache": true,
        "config.*": true,
        "configure": true,
        "install-sh": true,
        "aclocal.m4": true,
        "compile": true,
        "libtool": true,
        "ltmain.sh": true,
